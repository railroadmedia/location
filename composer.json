{
  "name": "railroad/location",
  "type": "library",
  "description": "Location service",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "~7",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "monarobase/country-list": "^2",
    "league/iso3166": "^3.0.0",
    "sokil/php-isocodes": "^3.0",
    "umpirsky/country-list": "^2.0.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "6.0.1",
    "orchestra/testbench": "~3.6",
    "filp/whoops":"~2.1"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Location\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Location\\Tests\\": "tests/"
    }
  }
}
